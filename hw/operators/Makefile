### Makefile for the selectiontest project
### Generated by Bluespec Workstation on Wed Apr 24 20:51:26 EDT 2013

default: full_clean compile link simulate

.PHONY: compile
compile:
	@echo Compiling...
	bsc -u -sim -simdir bscdir -bdir bscdir -info-dir bscdir -keep-fires -aggressive-conditions -p .:%/Prelude:%/Libraries:%/Libraries/BlueNoC:./../ddr:./../marshaller:./../proc:./../controller -g mkSelectionTest  SelectionTest.bsv 
	@echo Compilation finished

.PHONY: link
link:
	@echo Linking...
	bsc -e mkSelectionTest -sim -o ./out -simdir bscdir -p bscdir:.:%/Prelude:%/Libraries:%/Libraries/BlueNoC:./../ddr:./../marshaller:./../proc:./../controller -keep-fires 
	@echo Linking finished

.PHONY: simulate
simulate:
	@echo Simulation...
	#./out 
	@echo Simulation finished

.PHONY: clean
clean:
	rm -rf ./out ./out.so bscdir/*


.PHONY: full_clean
full_clean:
